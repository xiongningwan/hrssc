<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.maiyu.hrssc.home.activity.ChooseAddressActivity"
    android:orientation="vertical"
    android:background="@color/project_color_basic_white"
    >



   <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="@dimen/project_head_view_hight">
       <RelativeLayout
           android:id="@+id/Left_button_rl"
           android:layout_width="wrap_content"
           android:layout_height="match_parent"
           android:layout_marginStart="@dimen/activity_horizontal_margin">
           <ImageView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:src="@mipmap/icon_cha"
               android:layout_centerVertical="true"/>

       </RelativeLayout>

       <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="提示"
           android:layout_centerHorizontal="true"
           android:layout_centerVertical="true"
           android:textColor="@color/text_title_gray"
           android:textSize="@dimen/project_text_number_size_dp_19"/>


   </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@mipmap/line"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/icon_tijiaochenggong"
        android:layout_marginTop="@dimen/base88dp"
        android:layout_gravity="center_horizontal"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="提交审核成功"
        android:textColor="@color/text_title_gray"
        android:layout_marginTop="@dimen/base20dp"
        android:layout_gravity="center_horizontal"/>.


    <Button
        android:id="@+id/btn_back_home_work"
        android:layout_width="@dimen/base247dp"
        android:layout_height="@dimen/project_btn_hight"
        android:layout_marginTop="@dimen/base79dp"
        android:layout_gravity="center_horizontal"
        android:text="返回工作台"
        android:textColor="@color/project_color_basic_white_pure"
        android:background="@drawable/corner_view_selector"/>
</LinearLayout>
